{
  "name": "typed-selector",
  "description": "A simple, typesafe selector library.",
  "repository": {
    "url": "https://github.com/LaMavia/typed-selector"
  },
  "private": false,
  "version": "0.2.1",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/typed-selector.umd.cjs",
  "module": "./dist/typed-selector.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "types": "./dist/index.d.ts",
    "import": "./dist/typed-selector.js",
    "require": "./dist/typed-selector.umd.cjs"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "prettier": "prettier -w .",
    "prepack": "npm run build",
    "prepublish": "npm run test:coverage"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^0.33.0",
    "prettier": "^3.0.0",
    "typescript": "^5.1.3",
    "vite": "^4.3.9",
    "vite-plugin-dts": "^3.3.1",
    "vitest": "^0.33.0"
  }
}
